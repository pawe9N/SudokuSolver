<Page x:Class="SudokuSolver.Pages.StartPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:SudokuSolver.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="StartPage">

    <StackPanel VerticalAlignment="Center" Width="100">
        <Button Name="StartGame" Content="Start Game" Click="StartGame_Click"/>
        <Button Name="Solver" Content="Sudoku Solver" Click="Solver_Click"/>
    </StackPanel>
</Page>
