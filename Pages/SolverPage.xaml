<Page x:Class="SudokuSolver.Pages.SolverPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:SudokuSolver.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="SudokuSolver">

    <Grid>
        <Canvas Name="myCanvas">

        </Canvas>
        <Button Name="SolveButton" Style="{DynamicResource ResourceKey=SolveButtonStyle}" Click="SolveButton_Click"/>
        <Button Name="NewEmptySudokuButton" Style="{DynamicResource ResourceKey=NewButtonStyle}" Click="NewEmptySudokuButton_Click"/>
        <Button Name="BackButtone" Style="{DynamicResource ResourceKey=BackButtoneStyle}" Click="BackButtone_Click"/>
        <Label Name="ErrorText" Style="{DynamicResource ResourceKey=ErrorTextLabelStyle}"/>
    </Grid>
</Page>
