<Page x:Class="SudokuSolver.Pages.Game"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="SudokuSolver">

    <Grid>
        <Canvas Name="myCanvas"> 

        </Canvas>
        <Label Name="PointsText" Style="{StaticResource PointsTextLabelStyle}"/>
        <Button Name="SolveButton" Style="{StaticResource SolveSudokuButtonStyle}" Click="SolveButton_Click"/>
        <Button Name="CheckInputs" Style="{StaticResource CheckInputsButtonStyle}" Click="CheckInputs_Click"/>
        <Button Name="NextSudoku" Style="{StaticResource NextButtonStyle}" Click="NextSudoku_Click"/>
        <Button Name="BackButtone" Style="{StaticResource BackButtoneStyle}" Click="BackButtone_Click"/>
    </Grid>

</Page>
